{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src",
    "skipLibCheck": true,
    "outDir": "es",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@*": [
        "src/*"
      ],
      "@@/*": [
        "src/.umi/*"
      ]
    },
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "jest.config.js"
  ]
}
